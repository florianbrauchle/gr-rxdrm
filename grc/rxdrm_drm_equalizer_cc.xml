<?xml version="1.0"?>
<block>
  <name>Equalizer</name>
  <key>rxdrm_drm_equalizer_cc</key>
  <category>DRM-Rx</category>
  <import>import rxdrm</import>
  <make>rxdrm.drm_equalizer_cc($p_info)</make>


  <param>
    <name>Informationen Ausgeben</name>
    <key>p_info</key>
    <value>1</value>
    <type>int</type>
    <hide>part</hide>
    <option>
        <name>Nein</name>
        <key>0</key>
    </option>
    <option>
        <name>Ja</name>
        <key>1</key>
    </option>
  </param>



  <sink>
    <name>fac_in</name>
    <type>message</type>
  </sink>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  


  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
